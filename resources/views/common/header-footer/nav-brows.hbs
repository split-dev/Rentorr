{{#each nav-brows}}
    <div class="bg-white">
        <div class="container-box-full nav-position">
            <div class="nav-brows">
                <div class="nav-brows__logo-nav">
                    <a href="/" class="nav__logo">
                        <img src="{{logo}}" alt="logo">
                    </a>
                </div>
                <div class="nav-brows__search-position">
                    <div class="nav-brows__search">
                        <form>
                            <input type="text" placeholder="{{search-input}}">
                            <input type="text" placeholder="{{search-input-mobile}}">
                        </form>
                    </div>
                    <div class="nav__button">
                        <a href="#" class="nav__button--white button--white--border nav__sign-in">SIGN IN</a>
                        <a href="#" class="nav__button--blue button--blue nav__sign-up">SIGN UP</a>
                    </div>
                    <a href="#" class="nav__mobile-button">
                        <span></span>
                        <span></span>
                    </a>
                </div>
            </div>
        </div>
    </div>
{{/each}}
