<html lang="en">
<head>
    {{> ../common/head }}
</head>
<body class="landlord-one">
{{> ../common/mobile-menu}}
{{> ../common/nav }}

{{#each top-page}}
    {{> ../common/top-page }}
{{/each}}
{{> ../common/sign-in}}
{{> ../common/sign-up}}
{{> ../common/check-email}}
{{> ../common/forgot-pass}}
{{> ../common/select-role}}
{{#each paragraf-one}}
    {{> ../partials/paragraf }}
{{/each}}
<div class="circle">
{{#each circle}}
    <div class="circle__position wow fadeInUp" data-wow-duration="2s">
        <img src="{{circle-img}}" alt="img">
    </div>
{{/each}}
</div>
{{#each apply}}
    {{> ../common/apply}}
{{/each}}
<div class="what-land-one">
    {{#each title-one}}
        {{> ../partials/title-section }}
    {{/each}}
    {{#each feature-one}}
        {{> ../partials/features }}
    {{/each}}
</div>
{{#each ready}}
    {{> ../common/ready }}
{{/each}}
{{> ../common/footer }}
</body>
</html>
