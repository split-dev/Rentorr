
<html lang="en">
<head>
    {{> ../common/head }}
</head>
<body class="error">
{{> ../common/mobile-menu}}
{{> ../common/nav }}

{{#each top-page}}
    {{> ../common/top-page }}
{{/each}}
{{> ../common/sign-in}}
{{> ../common/sign-up}}
{{> ../common/check-email}}
{{> ../common/forgot-pass}}
{{> ../common/select-role}}
    <div class="container-box-full error__img-position">
{{#each error}}
        <img src="{{img}}" alt="img">
{{/each}}
    </div>
    <div class="container-box-full error__desc">
        <h3>Ooops!</h3>
        <span>Something went wrong</span>
        <p><a class="error__link second-link" href="#">Go back</a> or head over to <a class="error__link second-link" href="/">Home Page</a> to choose a new direction.</p>
    </div>
</body>
</html>

